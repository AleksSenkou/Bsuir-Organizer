.container-settings.col-xs-10.col-xs-offset-1.col-md-4.col-md-offset-3
  %body
    %label График работы:
    .input-group-timetable
      %input.form-control{:placeholder => "8:00", :type => "text"}/
      %input.form-control{:placeholder => "22:00", :type => "text"}/
    %label Время от дома до работы:
    .input-group
      %input.form-control{:placeholder => "40", :type => "text"}/
      %span.input-group-addon минуты
    %label от работы до университета:
    .input-group
      %input.form-control{:placeholder => "30", :type => "text"}/
      %span.input-group-addon минуты
    %label от университета до дома:
    .input-group
      %input.form-control{:placeholder => "45", :type => "text"}/
      %span.input-group-addon минуты
    %label Минимальное время работы:
    .input-group
      %input.form-control{:placeholder => "120", :type => "text"}/
      %span.input-group-addon минуты
    %button.btn.btn-success{:type => "button"} Посчитать
